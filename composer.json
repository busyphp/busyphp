{
	"name"         : "busyphp/busyphp",
	"type"         : "library",
	"description"  : "基于ThinkPHP研发的一款可以高效开发B/S应用的框架",
	"homepage"     : "http://www.harter.cn/?form=BusyPHP",
	"keywords"     : [
		"BusyPHP",
		"框架",
		"后台管理系统"
	],
	"authors"      : [
		{
			"name"  : "busy^life",
			"email" : "busy.life@qq.com"
		}
	],
	"license"      : "CC-BY-NC-4.0",
	"require"      : {
		"php"                      : ">=7.2.5",
		"topthink/framework"       : "6.0.12",
		"topthink/think-orm"       : "2.0.52",
		"topthink/think-multi-app" : "1.0.14",
		"topthink/think-view"      : "1.0.14",
		"topthink/think-trace"     : "1.4",
		"james-heinrich/phpthumb"  : "1.7.15",
		"aferrandini/phpqrcode"    : "1.0.1",
		"psr/container"            : "1.1.1",
		"ext-json"                 : "*",
		"ext-pdo"                  : "*",
		"ext-iconv"                : "*",
		"ext-gd"                   : "*",
		"ext-curl"                 : "*",
		"ext-zip"                  : "*",
		"ext-fileinfo"             : "*",
		"ext-openssl"              : "*"
	},
	"require-dev"  : {
		"symfony/var-dumper" : "^4.2",
		"phpunit/phpunit"    : "^7.0"
	},
	"autoload"     : {
		"psr-4" : {
			"BusyPHP\\" : "src"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"BusyPHP\\tests\\" : "tests"
		}
	},
	"config"       : {
		"preferred-install" : "dist"
	},
	"extra"        : {
		"branch-alias" : {
			"dev-busyphp6" : "6.0.x-dev"
		},
		"busyphp"      : {
			"manager"     : {
				"name" : "BusyPHP"
			},
			"services"    : [
			],
			"initializes" : [
			],
			"extract"     : {
			},
			"config"      : {
			}
		}
	}
}
