{
	"name"        : "busyphp/busyphp",
	"type"        : "library",
	"description" : "基于ThinkPHP研发的一款可以高效开发B/S应用的框架",
	"keywords"    : [
		"BusyPHP",
		"框架",
		"后台管理系统"
	],
	"authors"     : [
		{
			"name"  : "busy^life",
			"email" : "busy.life@qq.com"
		}
	],
	"license"     : "CC-BY-NC-4.0",
	"require"     : {
		"php"                      : ">=7.1.0",
		"topthink/framework"       : "6.0.7",
		"topthink/think-orm"       : "2.0.39",
		"topthink/think-multi-app" : "1.0.14",
		"topthink/think-view"      : "1.0.14",
		"topthink/think-trace"     : "1.3",
		"james-heinrich/phpthumb"  : "1.7.15",
		"aferrandini/phpqrcode"    : "1.0.1",
		"ext-json"                 : "*",
		"ext-pdo"                  : "*",
		"ext-iconv"                : "*",
		"ext-gd"                   : "*",
		"ext-curl"                 : "*",
		"ext-zip"                  : "*",
		"ext-fileinfo"             : "*",
		"ext-openssl"              : "*"
	},
	"require-dev" : {
		"symfony/var-dumper" : "^4.2"
	},
	"autoload"    : {
		"psr-4" : {
			"BusyPHP\\" : "src"
		}
	},
	"config"      : {
		"preferred-install" : "dist"
	},
	"extra"       : {
		"busyphp" : {
			"manager"     : {
				"name" : "BusyPHP"
			},
			"services"    : [
			],
			"initializes" : [
			],
			"extract"     : {
			},
			"config"      : {
			}
		}
	}
