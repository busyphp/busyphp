{
	"name"         : "busyphp/busyphp",
	"type"         : "library",
	"description"  : "基于ThinkPHP研发的一款可以高效开发B/S应用的框架",
	"homepage"     : "http://www.harter.cn/?form=BusyPHP",
	"keywords"     : [
		"BusyPHP",
		"框架",
		"后台管理系统"
	],
	"authors"      : [
		{
			"name"  : "busy^life",
			"email" : "busy.life@qq.com"
		}
	],
	"license"      : "CC-BY-NC-4.0",
	"require"      : {
		"php"                       : ">=7.2.5",
		"ext-json"                  : "*",
		"ext-pdo"                   : "*",
		"ext-iconv"                 : "*",
		"ext-gd"                    : "*",
		"ext-curl"                  : "*",
		"ext-zip"                   : "*",
		"ext-fileinfo"              : "*",
		"ext-openssl"               : "*",
		"psr/container"             : "1.1.1",
		"guzzlehttp/guzzle"         : "7.5.0",
		"topthink/framework"        : "6.1.1",
		"topthink/think-filesystem" : "2.0.0",
		"topthink/think-orm"        : "2.0.54",
		"topthink/think-view"       : "1.0.14",
		"topthink/think-trace"      : "1.5",
		"intervention/image"        : "2.7.2",
		"bacon/bacon-qr-code"       : "2.0.7",
		"php-di/phpdoc-reader"      : "^2.2"
	},
	"require-dev"  : {
		"symfony/var-dumper" : "^4.2",
		"phpunit/phpunit"    : "^7.0"
	},
	"autoload"     : {
		"psr-4"    : {
			"BusyPHP\\"             : "src",
			"BusyPHP\\app\\"        : "application",
			"Intervention\\Image\\" : "src/third/extend/intervention/image"
		},
		"classmap" : [
			"src/third/override"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"BusyPHP\\tests\\" : "tests"
		}
	},
	"config"       : {
		"preferred-install" : "dist"
	},
	"extra"        : {
		"branch-alias" : {
			"dev-busyphp6" : "6.0.x-dev"
		},
		"busyphp"      : {
			"manager"     : {
				"name" : "BusyPHP"
			},
			"services"    : [
			],
			"initializes" : [
			],
			"extract"     : {
			},
			"config"      : {
			}
		}
	}
}
